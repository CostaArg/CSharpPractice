
@{
    ViewBag.Title = "Index";
    int a = 1;
    int b = 5;
    int sum = 0;
}

<h2>Index</h2>

<i>@a  @b</i>

@if (a > b)
{
    sum = a + b;
    <h1>@sum</h1>
}
else
{
    sum = a - b;
    <h1>@sum</h1>
}

<table class="table">
    <tbody>

        @foreach (var item in ViewBag.Students)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.FirstName</td>
            </tr>
        }

    </tbody>
</table>
